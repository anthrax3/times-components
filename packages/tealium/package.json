{
  "name": "@times-components/tealium",
  "version": "0.2.17",
  "description": "React storybook helpers for Times Components",
  "main": "dist",
  "dev": "src",
  "scripts": {
    "fmt": "prettier --write '**/*.*'",
    "prettier:diff": "prettier --list-different '**/*.*'",
    "lint": "eslint . && yarn prettier:diff",
    "test:web": "jest --config='./src/__tests__/web/jest.config.js'",
    "prepublishOnly": "yarn transpile",
    "transpile": "babel src --ignore node_modules,__tests__ -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newsuk/times-components.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/newsuk/times-components/issues"
  },
  "homepage": "https://github.com/newsuk/times-components#readme",
  "devDependencies": {
    "@storybook/addon-actions": "3.3.15",
    "@times-components/eslint-config-thetimes": "0.1.7",
    "@times-components/jest-configurator": "0.2.5",
    "@times-components/utils": "0.6.4",
    "babel-plugin-flow-react-proptypes": ">=14.0.3",
    "babel-plugin-add-react-displayname": ">=0.0.5",
    "babel-plugin-styled-components": ">=1.5.1",
    "babel-preset-react-native": ">=4.0.0",
    "eslint": "4.9.0",
    "jest": "21.2.1",
    "prettier": "1.8.2",
    "react-native-web": "0.3.2"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "react-native": ">=0.50",
    "react-native-web": ">=0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
